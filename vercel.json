{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "excelsiaedu-admin-system/build"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/excelsiaedu-admin-system/index.html"
    }
  ]
}
