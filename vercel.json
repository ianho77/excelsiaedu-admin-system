{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/test",
      "destination": "/api/test.js"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/excelsiaedu-admin-system/index.html"
    }
  ]
}
